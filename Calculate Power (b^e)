public class Main{
    public static int power(int b,int e){
        if(e==0){
            return 1;
        }
        if(e%2==0){
            int local=power(b,e/2);
            return local*local;
        }
        else{
            int local=power(b,e/2);
            return local*local*b;
        }
    }
    public static void main(String[] args){
        System.out.println(power(3,5));
    }
}
