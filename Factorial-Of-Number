public class Main{
    public static  int factorial(int n){
        if(n==0){
            return 1;
        }
        int res=n*factorial(n-1);
        return res;
    }
    public static void main(String[] args){
        int n=10;
        System.out.println("Factorial of "+n+" :"+factorial(n));
    }
    
}
